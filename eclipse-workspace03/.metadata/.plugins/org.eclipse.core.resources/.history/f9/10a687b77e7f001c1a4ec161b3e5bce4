package project2;

class Cuboid {
	static int r,l,b,h;
	
	static 
	{
		System.out.println("another block");
	}
	
	static
	{
		l=10;
		b=11;
		h=12;
		System.out.println("this is the block");
	}
	
	static void Calc()
	{
		r=l*b*h;
		System.out.println("this is the static method");
		System.out.println(r);
	}

}
