package constructor1;

public class Outer {
	private int data=30;//instance variable  
	 void display(){  
	  class Local
	{  
	   void msg()
	{
	System.out.println(data);
	}  
	 }  
	  Local l=new Local();  
	  l.msg();  
	 }  
	public static void main(String[] args) {
		Outer obj=new Outer();  
		  obj.display();  
		 }  

	}
